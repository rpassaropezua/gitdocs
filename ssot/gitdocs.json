{
  "$schema": "https://example.com/cmcc-schema/v1",
  "model_name": "GitCommitExporter",
  "Description": "System for exporting Git commits and linking to ClickUp tasks",
  "repositories": {
    "Description": "Git repositories to export commits from",
    "schema": [
      {
        "name": "RepositoryID",
        "datatype": "string",
        "type": "raw",
        "Description": "Unique identifier for the repository"
      },
      {
        "name": "RepositoryName",
        "datatype": "string",
        "type": "raw",
        "Description": "Name of the Git repository"
      },
      {
        "name": "Status",
        "datatype": "string",
        "type": "raw",
        "Description": "Current status of the repository"
      }
    ],
    "data": [
      {
        "RepositoryID": "R001",
        "RepositoryName": "gitdocs",
        "Status": "active"
      },
      {
        "RepositoryID": "R002",
        "RepositoryName": "gitutils",
        "Status": "inactive"
      }
    ]
  },
  "commits": {
    "Description": "Commits made to the repositories",
    "schema": [
      {
        "name": "CommitID",
        "datatype": "string",
        "type": "raw",
        "Description": "Unique identifier for the commit"
      },
      {
        "name": "RepositoryID",
        "datatype": "string",
        "type": "raw",
        "Description": "FK to repositories.RepositoryID"
      },
      {
        "name": "CommitDate",
        "datatype": "datetime",
        "type": "raw",
        "Description": "Date and time of the commit"
      },
      {
        "name": "CommitMessage",
        "datatype": "string",
        "type": "raw",
        "Description": "Message associated with the commit"
      },
      {
        "name": "ClickUpTaskLink",
        "datatype": "string",
        "type": "calculated",
        "formula": "=\"https://app.clickup.com/t/\" & MID({{CommitMessage}}, FIND(\"CU-\", {{CommitMessage}}), 8)",
        "Description": "Link to the ClickUp task extracted from the commit message"
      }
    ],
    "data": [
      {
        "CommitID": "C001",
        "RepositoryID": "R001",
        "CommitDate": "2023-10-01T12:34:56Z",
        "CommitMessage": "Fixed bug CU-12345",
        "ClickUpTaskLink": "https://app.clickup.com/t/CU-12345"
      },
      {
        "CommitID": "C002",
        "RepositoryID": "R001",
        "CommitDate": "2023-10-02T14:22:10Z",
        "CommitMessage": "Added feature CU-67890",
        "ClickUpTaskLink": "https://app.clickup.com/t/CU-67890"
      }
    ]
  }
}